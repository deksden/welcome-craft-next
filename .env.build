# Временный .env для production build testing - PHOENIX PROJECT
AUTH_SECRET=phoenix-build-secret-key-for-testing-purposes-only
NEXTAUTH_URL=http://localhost:3000

# App Stage для Phoenix системы
APP_STAGE=LOCAL
NODE_ENV=development

# Минимальные настройки для сборки (используются фиктивные значения)
XAI_API_KEY=fake-key-for-build
BLOB_READ_WRITE_TOKEN=fake-token-for-build
POSTGRES_URL=postgresql://fake:fake@localhost:5432/fake

# Redis (не обязательно для сборки)
REDIS_URL=redis://localhost:6379

# Webpack оптимизация
WEBPACK_LOGGING=false
NEXT_TELEMETRY_DISABLED=1
DEBUG=""