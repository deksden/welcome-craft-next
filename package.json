{
  "name": "welcome-craft-next",
  "appName": "WelcomeCraft",
  "version": "0.1.10",
  "private": true,
  "description": "Welcome Craft  -  AI based onboarding page management app",
  "type": "module",
  "keywords": [
    "welcome",
    "craft",
    "ai",
    "onboarding",
    "hr",
    "hr tools",
    "page builder",
    "website builder"
  ],
  "author": "(c) fab33, by deksden",
  "bugs": {
    "url": "https://github.com/deksden/welcome-craft-next/issues"
  },
  "homepage": "https://github.com/deksden/welcome-craft-next#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "pnpm db:migrate && next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint && biome lint --write --unsafe",
    "lint:fix": "next lint --fix && biome lint --write --unsafe",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "repomix:working": "repomix -c repomix.working.config.json",
    "repomix:all": "repomix",
    "setup": "bash ./setup.sh",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "npx tsx lib/db/migrate.ts",
    "db:studio": "drizzle-kit studio",
    "db:push": "drizzle-kit push",
    "db:pull": "drizzle-kit pull",
    "db:check": "drizzle-kit check",
    "db:up": "drizzle-kit up",
    "test": "playwright test",
    "test:unit": "vitest run",
    "test:routes": "playwright test --project=routes",
    "test:e2e": "playwright test --project=e2e",
    "test:ui": "playwright test --ui",
    "test:ci": "playwright test",
    "test:validate-worlds": "node scripts/validate-worlds.js",
    "world:load": "npx tsx scripts/load-world.ts",
    "world:list": "npx tsx scripts/load-world.ts --list",
    "test:db-up": "docker-compose up -d --wait",
    "test:db-down": "docker-compose down",
    "test:db-setup": "pnpm exec tsx scripts/setup-test-db.ts",
    "env:local": "cp .env.example .env.local && echo 'LOCAL environment configured'",
    "env:beta": "cp .env.beta .env.local && echo 'BETA environment configured'",
    "env:prod": "cp .env.prod .env.local && echo 'PROD environment configured'",
    "db:local:up": "docker-compose -f docker-compose.dev.yml up -d --wait",
    "db:local:down": "docker-compose -f docker-compose.dev.yml down",
    "db:local:logs": "docker-compose -f docker-compose.dev.yml logs -f",
    "db:local:reset": "docker-compose -f docker-compose.dev.yml down -v && docker-compose -f docker-compose.dev.yml up -d --wait",
    "db:beta:up": "docker-compose -f docker-compose.beta.yml up -d --wait",
    "db:beta:down": "docker-compose -f docker-compose.beta.yml down",
    "db:beta:logs": "docker-compose -f docker-compose.beta.yml logs -f",
    "db:beta:reset": "docker-compose -f docker-compose.beta.yml down -v && docker-compose -f docker-compose.beta.yml up -d --wait",
    "phoenix:local": "pnpm env:local && pnpm db:local:up && pnpm db:migrate && echo 'LOCAL environment ready!'",
    "phoenix:beta": "pnpm env:beta && pnpm db:beta:up && pnpm db:migrate && echo 'BETA environment ready!'",
    "phoenix:dev": "pnpm phoenix:local && pnpm dev",
    "phoenix:status": "echo 'PHOENIX PROJECT - Environment Status:' && docker ps --filter 'name=welcomecraft' --format 'table {{.Names}}\t{{.Status}}\t{{.Ports}}'",
    "phoenix:cleanup": "docker-compose -f docker-compose.dev.yml down && docker-compose -f docker-compose.beta.yml down && docker-compose down && echo 'All PHOENIX environments stopped'"
  },
  "dependencies": {
    "@ai-sdk/google": "^1.2.19",
